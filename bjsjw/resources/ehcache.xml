<ehcache name="complat_cache" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd">
	<diskStore path="java.io.tmpdir" />
	<defaultCache 
		eternal="true" 
		overflowToDisk="false"
	/>
