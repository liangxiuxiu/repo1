<?xml version="1.0" encoding="UTF-8" standalone="no"?><templates><template autoinsert="true" context="jsp_new" deleted="false" description="列表页模板" enabled="true" name="list_page">&lt;%@page language="java" contentType="text/html; charset=UTF-8"&#13;
	pageEncoding="UTF-8"%&gt;&#13;
&lt;%@taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c"%&gt;&#13;
&lt;%@taglib prefix="h" uri="/hanweb-tags"%&gt;&#13;
&lt;!DOCTYPE html&gt;&#13;
&lt;html&gt;&#13;
&lt;head&gt;&#13;
&lt;meta http-equiv="X-UA-Compatible" content="IE=edge" /&gt;&#13;
&lt;meta http-equiv="Content-Type" content="text/html; charset=UTF-8" /&gt;&#13;
&lt;title&gt;&lt;!--标题--&gt;&lt;/title&gt;&#13;
&lt;h:head pagetype="page" grid="true"&gt;&lt;/h:head&gt;&#13;
&lt;!--使用$${contextPath}可以获得应用路径，并无/结尾--&gt;&#13;
&lt;script&gt;&#13;
	/**&#13;
	* 列表页功能&#13;
	*/&#13;
	function toolbarAction(action) {&#13;
		switch (action) {&#13;
		case 'remove':&#13;
			var ids = getCheckedIds();&#13;
			if (ids == '') {&#13;
				alert('未选中任何记录');&#13;
				return;&#13;
			}&#13;
			confirm('您确定要删除这' + ids.split(',').length + '条记录吗', function() {&#13;
				ajaxSubmit('remove.do?ids='+ids);&#13;
			});&#13;
			break;&#13;
		case 'add':&#13;
			openDialog(填写新增url, 550, 560, {&#13;
				title : '新增'&#13;
			});&#13;
			break;&#13;
		}&#13;
	}&#13;
&#13;
	function edit(iid, name) {&#13;
		openDialog(填写新增url, 550, 560, {&#13;
			title : '修改'&#13;
		});&#13;
	}&#13;
&lt;/script&gt;&#13;
&lt;/head&gt;&#13;
&lt;body&gt;&#13;
	&lt;div id="page-title"&gt;&#13;
		&lt;span class="btn switchmenu"&gt;&lt;i class="icon-sitemap"&gt;&lt;/i&gt; &lt;!--模块名称 --&gt;&#13;
			- &lt;span id="page-title-text"&gt;&lt;!--子名称（可以没有） --&gt;&lt;/span&gt; &lt;i class="icon-caret-down"&gt;&lt;/i&gt;&#13;
		&lt;/span&gt;&#13;
	&lt;/div&gt;&#13;
	&lt;div id="page-content"&gt;&#13;
    	&lt;!--高级检索开始form无需url --&gt;&#13;
		&lt;div class="grid-advsearch"&gt;&#13;
			&lt;form&gt;&#13;
            	&lt;!--&#13;
				机构名称&lt;input name="name" type="text" class="input-text" value="${name }" style="width: 120px; margin: 0 30px 0 10px;" /&gt; &#13;
				机构标识&lt;input name="codeId" type="text" class="input-text" value="${codeId }" style="width: 120px; margin: 0 30px 0 10px;" /&gt;&#13;
				&lt;input type="button" class="btn btn-small btn-primary" value="检索" /&gt;&#13;
				&lt;input type="button" class="btn btn-small advsearch-cancel" value="返回" /&gt;&#13;
				&lt;div class="line-dotted"&gt;&lt;/div&gt;&#13;
                --&gt;&#13;
			&lt;/form&gt;&#13;
		&lt;/div&gt;&#13;
        &lt;!--高级检索结束 --&gt;&#13;
		&lt;h:grid&gt;&lt;/h:grid&gt;&#13;
	&lt;/div&gt;&#13;
&lt;/body&gt;&#13;
&lt;/html&gt;</template></templates>